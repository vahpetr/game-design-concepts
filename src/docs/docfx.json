{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "media/**",
          "robots.txt"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "globalMetadata": {
      "_lang": "ru",
      "_appName": "Игровой дизайн",
      "_appTitle": "Игровой дизайн",
      "_enableSearch": true,
      "pdf": true
    },
    "sitemap": {
      "baseUrl": "https://vahpetr.github.io/game-design-concepts",
      "priority": 0.1,
      "changefreq": "monthly"
    }
  }
}